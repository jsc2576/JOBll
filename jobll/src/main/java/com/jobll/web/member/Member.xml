<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobll.web.member.Member">

	<select id="select" resultType="java.util.HashMap">
		SELECT * FROM "meminfo"
	</select>
	
	
	<select id="read" parameterType="com.jobll.web.member.Member" resultType="java.util.HashMap">
		SELECT * FROM "mem_info" WHERE mem_idx = #{mem_idx}
	</select>
		
	<insert id="create" parameterType="com.jobll.web.member.Member">
		insert into "meminfo"(
			"mem_idx",
			"mem_id",
			"mem_pw",
			"mem_nm",
			"mem_adress",
			"mem_phone",
			"mem_type",
			"mem_grade",
			"mem_company",
			"mem_valid"
			)
		values (
			(select nextval('seq_mem_idx')),
			#{mem_id},
			#{mem_pw},
			#{mem_nm},
			#{mem_adress},
			#{mem_phone},
			#{mem_type},
			#{mem_grade},
			#{mem_company},
			#{mem_valid}
			)
	</insert>
	
	<update id="edit" parameterType="com.jobll.web.member.Member">
		update "meminfo" set
		        
		where "mem_idx" = #{mem_idx}
	</update>
	
	<delete id="delete" parameterType="com.jobll.web.member.Member">
		delete from "meminfo"        
		where "mem_idx" = #{mem_idx}
	</delete>
</mapper>
