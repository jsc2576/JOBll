<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobll.web.home.Home">

	<insert id="create" parameterType="com.jobll.web.home.Home">
		insert into testdb(
			test_idx
		)
		values (
			
			#{test_idx}
		)
	</insert>
	
	<select id="findOne" parameterType="com.jobll.web.home.Home" resultType="com.jobll.web.home.Home">
		select
			*
		from testdb
		where
		test_idx = #{test_idx}
	</select>

</mapper>