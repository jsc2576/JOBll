<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobll.web.dashbrd.DashBrd">

	<select id="findPrjtIdx" parameterType="Integer" resultType="java.util.HashMap">
		select prjt_idx where usr_id = #{usr_id}
	</select>
	
	<select id="findPrjt" parameterType="Integer" resultType="java.util.String">
		select prjt_sbjt
		<where>
			prjt_idx IN
			<foreach collection="prjt_idx" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
		</where>
	</select>
</mapper>