<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jobll.web.login">
<select id="select" parameterType="java.lang.String" resultType="com.jobll.web.domain.UserVo">

SELECT usr_id AS userId, usr_pwd AS passWd
FROM usr_info
where usr_id = #{userId} LIMIT 1
</select>

<select id="sel" parameterType="java.lang.String" resultType="com.jobll.web.usrinfo.UsrInfo">

SELECT * FROM usr_info
where usr_id = #{userId} LIMIT 1
</select>
</mapper>